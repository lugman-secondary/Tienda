/**
 * @file
 * JavaScript file for seach popup.
 */

(function ($, Drupal, window, document, undefined) {
  Drupal.behaviors.fostersPopupSearchPopup = {
    attach: function(context, settings) {

      // Shows the popup with colorbox library.
      $('.popup-search').once('fosters-popup', function() {
        $(this).colorbox({
          inline: true,
          href: '#block-fosters-popup-search-fh-popup-search-block',
          width: '100%',
          maxWidth: '776px',
          close: Drupal.t('Close'),
          transition: 'fade',
          className: 'cb-search',
          fixed: true
        });

        // Depending where the popup is triggered, it has different result behavior,
        // If is launched in a carte link and the user selects a restaurant througt
        // the form, it has to go to the restaurant node and go the carte section
        // filtering by the family option choosed in the previous page.
        // Example: Viewing carte meat, goes to restaurant carte meat section.
        $(this).click(function() {
          var category = $(this).attr('data-cat'),
              service = $(this).attr('data-service'),
              form = $('.form-popup-search');

          goog_report_conversion();

          // Category.
          if (typeof category != 'undefined') {
            form.attr('data-cat', category);
          }
          else {
            form.removeAttr('data-cat');
          }

          // Services.
          if (typeof service != 'undefined') {
            form.find('.form-item-services select').val(service).change();
          }
          else {
            form.find('.form-item-services select').val('').change();
          }
        });
      });

      if ($('.form-popup-search .form-item-switcher-popup input').is(':checked')) {
        $('.form-popup-search .form-type-select.form-item-province').hide();
        $('.form-popup-search .form-type-select.form-item-locality').hide();
        $('.form-popup-search .form-type-select.form-item-restaurant').hide();
      }

      preventSubmitOnAjaxProgress('.form-popup-search', context);

    }
  };

  /**
   * Set all fields in form disabled when ajax is in progress.
   *
   * @param string selector
   * @param array context
   */
  function preventSubmitOnAjaxProgress (selector, context){
    $(document).ajaxStart(function() {
      $(selector).find('input, select').each(function (){
        $(this).attr('disabled', 'disabled');
      });
    });
    $(document).ajaxComplete(function() {
      $(selector).find('input, select').each(function (){
        $(this).removeAttr('disabled');
      });
    });
  }
})(jQuery, Drupal, this, this.document);
