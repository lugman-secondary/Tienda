(function($) {
  Drupal.behaviors.fostersPopupSearchGeocoder = {
    attach : function(context, settings) {

      $('.form-popup-search').once('fosters-popup-form', function() {
        var form = $(this),
            switcher = form.find('.form-item-switcher-popup input'),
            label = form.find('.form-item-switcher-popup label');

        // Change label text depending of checkbox value.
        switcher.change(function() {
          jQuery().colorbox.resize()

          if (switcher.is(':checked')) {
            label.html(Drupal.t('Or search by restaurant ...'));
          }
          else {
            label.html(Drupal.t('Or search by Postal code, address ...'));
          }
        });

        form.submit(function(event) {
          var category = form.attr('data-cat');

          if (typeof category != 'undefined') {
            form.find('input[name="fragment"]').val('ctab-' + category);
          }
        });
      });

    }
  };
})(jQuery);
